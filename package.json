{
  "name": "trpc-basic-serverless",
  "version": "0.0.0",
  "scripts": {
    "dev:client": "vite",
    "build:client": "tsc && vite build",
    "preview:client": "vite preview",
    "build:backend": "swc backend -d backend-output --config-file .swcrc",
    "dev:backend": "concurrently \"swc backend -d backend-output --watch\" \"nodemon -q backend-output/functions/api.js\"",
    "preview:backend": "backend-output/functions/api.js",
    "serverful": "concurrently \"cross-env SERVERFUL=true vite\" \"npm run dev:backend\"",
    "build": "npm run build:client && npm run build:backend",
    "start": "concurrently \"npm run dev:client\" \"npm run preview:backend\"",
    "netlify:dev": "netlify dev"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "@tanstack/react-query": "^4.12.0",
    "@trpc/client": "^10.0.0-proxy-beta.21",
    "@trpc/react-query": "^10.0.0-proxy-beta.21",
    "@trpc/server": "^10.0.0-proxy-beta.21",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "serverless-http": "^3.0.3",
    "styled-components": "^5.3.6",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.9",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node-fetch": "^2.6.2",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20",
    "tailwindcss": "^2.2.19",
    "twin.macro": "^2.8.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  }
}